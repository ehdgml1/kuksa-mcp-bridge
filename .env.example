# Kuksa Databroker
KUKSA_DATABROKER_HOST=databroker
KUKSA_DATABROKER_PORT=55555

# Simulator
SIM_MODE=normal_driving
SIM_UPDATE_INTERVAL_MS=500
SIM_LOG_LEVEL=INFO

# MCP Server
MCP_SERVER_NAME=kuksa-vehicle-bridge
MCP_LOG_LEVEL=INFO

# DBC Feeder (Phase 1-B)
