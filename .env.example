# Options: hyundai_kia | tesla_model3
VEHICLE_PROFILE=hyundai_kia

# Dashboard
VITE_KUKSA_WS_URL=ws://localhost:8090

# Agent Server (Phase 3)
GEMINI_API_KEY=your-gemini-api-key-here
AGENT_LOG_LEVEL=INFO
